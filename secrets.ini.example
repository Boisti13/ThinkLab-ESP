; secrets.ini.example

[secrets]
; ==============================
; Wi-Fi credentials
; ==============================
; Replace "YOURWIFI" with your actual SSID.
; Wrap it in quotes since it will be passed as a build flag.
build_flags = 
  -DWIFI_SSID='"YOURWIFI"'

; Replace "YOURWIFIPASSWORD" with your actual Wi-Fi password.
; Note: escaped quotes (\" \") are required inside the build flag.
  -DWIFI_PASS=\"YOURWIFIPASSWORD\"

; ==============================
; Hostname
; ==============================
; Device hostname, used for OTA and mDNS discovery.
  -DHOSTNAME=\"ThinkLab-esp32\"

; ==============================
; Web UI credentials
; ==============================
; Username for accessing the built-in web interface.
  -DWEB_USER=\"admin\"

; Password for accessing the built-in web interface.
  -DWEB_PASS=\"otapass\"
